{
  "name": "@plgd/shared-ui",
  "version": "1.1.0",
  "private": true,
  "author": "plgd",
  "description": "React components that implement plgd's design.",
  "main": "./src/index.js",
  "keywords": [
    "react",
    "react-component",
    "plgd"
  ],
  "scripts": {
    "build": "npm-run-all prebuild build:legacy build:modern build:node build:stable build:umd build:copy-files",
    "build:legacy": "node ./scripts/build legacy",
    "build:modern": "node ./scripts/build modern",
    "build:node": "node ./scripts/build node",
    "build:stable": "node ./scripts/build stable",
    "build:stable:watch": "node ./scripts/build stable --watch",
    "build:copy-files": "node ./scripts/copy-files.js",
    "build:umd": "rollup -c ./scripts/rollup.config.js",
    "lint": "npm-run-all :lint:eslint :lint:prettier",
    ":lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx -c .eslintrc.js --max-warnings 0 --fix --format=pretty ./src",
    ":lint:prettier": "prettier ** --check",
    "prebuild": "rimraf build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "icons:create": "npx @svgr/cli  --icon  --typescript -d ./src/components/new/Icon/components ./src/components/new/Icon/assets --config-file ./config/svgr-config.json --template ./config/svg-template.js",
    "icons:create:tmp": "npx @svgr/cli  --icon  --typescript -d ./src/components/new/Notification/components ./src/components/new/Notification/assets --config-file ./config/svgr-config.json --template ./config/svg-template.js"
  },
