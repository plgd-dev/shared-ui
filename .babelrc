{
  "presets": [
    [
      "@babel/preset-env",
      {
        "bugfixes": true,
        "debug": true,
        "modules": false
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    [
      "react-app",
      {
        "flow": false,
        "typescript": true,
        "helpers": false
      }
    ],
    [
      "@emotion/babel-preset-css-prop",
      {
        "autoLabel": "dev-only",
        "labelFormat": "[local]",
        "useBuiltIns": false,
        "throwNamespace": true
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime"]
  ],
  "comments": false
}


